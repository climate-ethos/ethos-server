# Docker account password
DOCKER_USER_PASSWORD=your_docker_user_password

# Domain name for SSH/HTTPS
DOMAIN_NAME=yourdomain.com

# Middleware auth username and password
USERNAME=user
PASSWORD=your_auth_password

# Twilio credentials
TWILIO_ACCOUNT_SID=your_twilio_account_sid
TWILIO_AUTH_TOKEN=your_twilio_auth_token
TWILIO_PHONE_NUMBER=your_twilio_phone_number
TWILIO_NOTIFY_SERVICE_SID=your_notify_service_sid

# CouchDB credentials
COUCHDB_ADMIN_USER=admin
COUCHDB_ADMIN_PASSWORD=your_couchdb_admin_password

# CouchDB settings
COUCHDB_URL=http://localhost:5984
